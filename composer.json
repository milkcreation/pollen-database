{
  "name": "pollen-solutions/database",
  "description": "Pollen Database Component",
  "homepage": "https://github.com/milkcreation/pollen-database.git",
  "keywords": [
    "pollen-solutions",
    "component",
    "database",
    "eloquent"
  ],
  "time": "2021-09-14",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "doctrine/dbal": "^3.1",
    "illuminate/database": "^8.0",
    "illuminate/pagination": "^8.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Database\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Database\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
